<div class="col-md-9 personal-info">
  <div class="alert alert-warning alert-dismissable">
    <a class="panel-close close" data-dismiss="alert">Ã—</a> 
    <strong>Warning:</strong> You can only <strong>submit</strong> your application when it is complete.
  </div>


  <div class="panel panel-default">
    <div class="panel-heading clearfix">
      <p>
        <h2 class="panel-title pull-left" style="padding-top: 7.5px;">Application</h2>
       <div align="right">
<%= link_to '<button style=" background-color: #fff" type="button" class="btn btn-default btn-xs"><span  class="glyphicon glyphicon-pencil"></span></button>'.html_safe, edit_profile_path(@profile.id)  %></p> 
</div>
</p>
</div>
<div class="panel-body">


  <div class="panel-group" id="accordion">
    <div class="panel panel-default" id="panel1">
      <div class="panel-heading">
        <h4 class="panel-title">
          <a data-toggle="collapse" data-target="#aboutmyself" 
          href="#aboutmyself">
          About Myself
        </a>
      </h4>
    </div>
    <div id="aboutmyself" class="panel-collapse collapse in">
      <%= render 'profiles/showAboutMyself' %>   
      </div>
    </div>
  </div>
  <div class="panel panel-default" id="panel2">
    <div class="panel-heading">
      <h4 class="panel-title">
        <a data-toggle="collapse" data-target="#PWNandAvailability" 
        href="#PWNandAvailability" class="collapsed">
        PWN, Mentoring and Availability
      </a>
    </h4>
  </div>
  <div id="PWNandAvailability" class="panel-collapse collapse">
    <%= render 'profiles/showPWNandAvailability' %>
  </div>
</div>
<div class="panel panel-default" id="panel3">
  <div class="panel-heading">
    <h4 class="panel-title">
      <a data-toggle="collapse" data-target="#DescriptionandInterest"
      href="#DescriptionandInterest" class="collapsed">
      Description of Myself and my Interest in the Program
    </a>
  </h4>
</div>
<div id="DescriptionandInterest" class="panel-collapse collapse">
<%= render 'profiles/showDescriptionandInterest' %>
</div>
</div>
</div>
</div>
</div>
</div>


